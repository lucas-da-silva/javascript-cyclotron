{
    "env": {
        "browser": true,
        "es2021": true
    },
    "plugins": [
        "prettier"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:node/recommended",
        "eslint-config-airbnb-base"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "no-console": "off"
    }
}